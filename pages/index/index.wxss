#bg-image {
  position: absolute;
  width: 100%;
  height: 100%;  
  z-index: -1;
}
#category-title {
  padding-top: 22px;
  margin-bottom: 22px;
  font-size: 28px;
  text-align: center;
}

#category-id-container {

}

.category-item {
  padding-top: 10px;
  padding-bottom: 10px;
  text-align: center;
  font-size: 22px;
}

#note {
  position: fixed;
  bottom: 0;
  padding: 10px;
}

#item-list-title {
  height: 32px;
  font-size: 18px;
}

.question-item {
  display: inline-block;
  width: 29vw;
  padding: 2vw;

}

.question-thumb {
  width: 29vw;
  height: 29vw;
  font-size: 18px;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}

#item-bottom-view {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(255, 255, 255, 0.7);
}

#navigateBack {
  padding: 2vw;
  display: inline-block;
  color: rgb(52, 143, 199);
}

#progress-info {
  display: inline-block;
  margin-left: 2vw;
}

#go {
  color: rgb(52, 143, 199);
  padding: 2vw;
  position: absolute;
  bottom: 0;
  right: 0;
}